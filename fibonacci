#include<stdio.h>
void main()
{
    int n,a=0,b=1,c;
    printf("the first two fibonacci numbers are %d and %d",a,b);
    printf("\nenter n:");
    scanf("%d",&n);
 printf("the finonacci upto %d is:",n);
    for(int i=2;i<=n;++i)
    {
        c=a+b;
        printf("%d\n",c);
        a=b;
        b=c;
    }
}    
    
