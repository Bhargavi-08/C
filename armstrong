#include<stdio.h>
void main()
{
    int n,sum=0,rem;
    printf("enter n:");
    scanf("%d",&n);
     int temp=n;
    while(n>0)
    {
        rem=n%10;
        sum=sum+(rem*rem*rem);
        n=n/10;
    }
    if(sum==temp)
    {
    printf("%d is an armstrong number",temp);
    }
    else
    {
        printf("%d is not an armstrong number",temp);
    }
}
